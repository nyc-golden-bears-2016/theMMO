<canvas id="game_screen" width="800" height="600"></canvas>

<script>
  var scheme = "ws://"
  <% if ENV["ENCRYPTED_WEBSOCKET"] == "true" %>
    scheme = "wss://"
  <% end %>
  setUpWebsocket(scheme);
  $('document').ready(function(){
    setAutoLogout();
    allowInput();
    setCharacterUpdate('<%= @username %>');
    setCanvasDrawingUpdate();
  });
</script>
